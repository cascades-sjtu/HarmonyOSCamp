{
  "app": {
    "bundleName": "com.ohos.demo.uitest",
    "vendor": "ohos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 4
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.ohos.demo.uitest",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.ohos.demo.uitest.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.ohos.demo.uitest.ControlAbility",
        "icon": "$media:icon",
        "description": "$string:controlability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      },
      {
        "name": "com.ohos.demo.uitest.ServiceAbility",
        "icon": "$media:icon",
        "description": "$string:serviceability_description",
        "type": "service"
      },
      {
        "orientation": "unspecified",
        "name": "com.ohos.demo.uitest.PreviewAbility",
        "icon": "$media:icon",
        "description": "$string:previewability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.DISTRIBUTED_DATASYNC",
        "reason": "多设备协同",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DEVICE_STATE_CHANGE",
        "reason": "获取设备状态变化",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO",
        "reason": "获取设备基本信息",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GRT_BUNDLE_INFO",
        "reason": "获取应用信息",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.READ_USER_STORAGE",
        "reason": "读存储",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.WRITE_USER_STORAGE",
        "reason": "写存储",
        "usedScene": {
          "ability": [
            ".MainAbility",
            ".PreviewAbility",
            ".ControlAbility",
            ".ServiceAbility"
          ],
          "when": "inuse"
        }
      }
    ]
  }
}